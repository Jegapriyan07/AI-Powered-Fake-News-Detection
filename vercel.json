{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/analyze",
            "dest": "/app.py"
        },
        {
            "src": "/bias",
            "dest": "/app.py"
        },
        {
            "src": "/sources",
            "dest": "/app.py"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}
